<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".resultdatails">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Lectures"
        android:textSize="20sp"
        android:textColor="#000"
        android:textStyle="bold"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="10dp"></TextView>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical">
        <androidx.cardview.widget.CardView
            android:layout_width="105dp"
            android:layout_height="200dp"
            android:layout_marginLeft="10sp"
            android:layout_marginTop="25sp"
            android:id="@+id/c1">
            <ImageView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:src="@drawable/physicspic"></ImageView>
        </androidx.cardview.widget.CardView>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Physics"
                android:textSize="17sp"
                android:layout_marginLeft="15dp"
                android:textColor="#000"></TextView>
        </LinearLayout>
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical">
        <androidx.cardview.widget.CardView
            android:layout_width="105dp"
            android:layout_height="200dp"
            android:id="@+id/c2"
            android:layout_marginLeft="13dp"
            android:layout_marginTop="25sp">
            <ImageView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:src="@drawable/urdu"></ImageView>
        </androidx.cardview.widget.CardView>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Urdu"
                android:textSize="17sp"
                android:layout_marginLeft="15dp"
                android:textColor="#000"></TextView>
        </LinearLayout>
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical">
        <androidx.cardview.widget.CardView
            android:layout_width="105dp"
            android:layout_height="200dp"
            android:id="@+id/c3"
            android:layout_marginLeft="13sp"
            android:layout_marginTop="25sp">
            <ImageView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:src="@drawable/maths"></ImageView>
        </androidx.cardview.widget.CardView>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Math"
                android:textSize="17sp"
                android:layout_marginLeft="15dp"
                android:textColor="#000"></TextView>
        </LinearLayout>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical">
            <androidx.cardview.widget.CardView
                android:layout_width="105dp"
                android:layout_height="200dp"
                android:id="@+id/c4"
                android:layout_marginLeft="10sp"
                android:layout_marginTop="15sp">
                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:src="@drawable/pakstudyremovebg"></ImageView>
            </androidx.cardview.widget.CardView>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Pak Study"
                android:textSize="17sp"
                android:layout_marginLeft="15dp"
                android:textColor="#000"></TextView>
        </LinearLayout>
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical">
            <androidx.cardview.widget.CardView
                android:layout_width="105dp"
                android:layout_height="200dp"
                android:layout_marginLeft="13dp"
                android:id="@+id/c5"
                android:layout_marginTop="15sp">
                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:src="@drawable/computerremovebg"></ImageView>
            </androidx.cardview.widget.CardView>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Computer"
                android:textSize="17sp"
                android:layout_marginLeft="15dp"
                android:textColor="#000"></TextView>
        </LinearLayout>
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical">
            <androidx.cardview.widget.CardView
                android:layout_width="105dp"
                android:layout_height="200dp"
                android:id="@+id/c6"
                android:layout_marginLeft="13sp"
                android:layout_marginTop="15sp">
                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:src="@drawable/eng"></ImageView>
            </androidx.cardview.widget.CardView>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="English"
                android:textSize="17sp"
                android:layout_marginLeft="15dp"
                android:textColor="#000"></TextView>
        </LinearLayout>
    </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical">
                <androidx.cardview.widget.CardView
                    android:layout_width="105dp"
                    android:layout_height="200dp"
                    android:layout_marginLeft="10sp"
                    android:id="@+id/c7"
                    android:layout_marginTop="15sp">
                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:src="@drawable/chemistry"></ImageView>
                </androidx.cardview.widget.CardView>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Chemistry"
                    android:textSize="17sp"
                    android:layout_marginLeft="15dp"
                    android:textColor="#000"></TextView>
            </LinearLayout>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical">
                <androidx.cardview.widget.CardView
                    android:layout_width="105dp"
                    android:layout_height="200dp"
                    android:layout_marginLeft="10sp"
                    android:id="@+id/c8"
                    android:layout_marginTop="15sp">
                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:src="@drawable/biology"></ImageView>
                </androidx.cardview.widget.CardView>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Biology"
                    android:textSize="17sp"
                    android:layout_marginLeft="15dp"
                    android:textColor="#000"></TextView>
            </LinearLayout>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical">
                <androidx.cardview.widget.CardView
                    android:layout_width="105dp"
                    android:layout_height="200dp"
                    android:id="@+id/c9"
                    android:layout_marginLeft="10sp"
                    android:layout_marginTop="15sp">
                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:src="@drawable/islambk"></ImageView>
                </androidx.cardview.widget.CardView>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Islamiyat"
                    android:textSize="17sp"
                    android:layout_marginLeft="15dp"
                    android:textColor="#000"></TextView>
            </LinearLayout>
        </LinearLayout>
        </LinearLayout>
</ScrollView>